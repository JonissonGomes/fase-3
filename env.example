# Configurações do Ambiente
NODE_ENV=development

# Configurações do MongoDB
MONGODB_URI=mongodb://admin:password@localhost:27017

# Configurações JWT
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production

# URLs dos Serviços
